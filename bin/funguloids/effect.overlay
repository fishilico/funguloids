// Special level transition overlay
overlay SpecialLevelTransition
{
	zorder 600

	overlay_element Effect Panel
	{
		metrics_mode relative
		left 0.0
		top 0.0
		width 1.0
		height 1.0
		material SpecialLevelTransition
	}
}


// Ball worm glow overlay
overlay BallWormGlow
